{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "palm",
  "name": "Laxi.ai Documentation",
  "appearance": {
    "default": "light"
  },
  "colors": {
    "primary": "#6e51e0",
    "light": "#6e51e0",
    "dark": "#6e51e0"
  },
  "background": {
    "color": {
      "dark": "#0a0814"
    },
    "decoration": "windows"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "User Guides",
        "groups": [
          {
            "group": "Get Started",
            "pages": ["about", "quickstart"]
          },
          {
            "group": "Learn",
            "pages": [
              "learn/design",
              "learn/data-sources",
              "learn/playground",
              "learn/model-controls"
            ]
          },
          {
            "group": "Deploy",
            "pages": [
              "deploy/manual-embed", 
              "deploy/shopify"
            ]
          },
          {
            "group": "Monitor",
            "pages": [
              "analytics/conversations", 
              "analytics/dashboard"
            ]
          },
          {
            "group": "Support",
            "pages": [
              "support/email-support"
            ]
          },
          {
            "group": "Account",
            "pages": [
              "account/profile",
              "account/subscriptions",
              "account/billing"
            ]
          }
        ]
      },
      {
        "tab": "Integrations",
        "groups": [
          {
            "group": "Platform Setup",
            "pages": [
              "setup/general",
              "setup/shopify"
            ]
          }
        ]
      }
    ],
    "global": {}
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:team@laxi.ai"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Try laxi.ai",
      "href": "https://laxi.ai"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/laxi_ai",
      "github": "https://github.com/laxi-ai",
      "linkedin": "https://linkedin.com/company/laxi-ai"
    }
  }
}
